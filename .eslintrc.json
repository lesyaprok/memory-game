{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [ 
        "eslint:recommended",
        "plugin:react/recommended",
        "airbnb",
        "prettier"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "babelOptions": {
          "presets": ["@babel/preset-react"]
        },
        "ecmaVersion": 2021,
        "requireConfigFile": false,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "error", 
        "react/prop-types": 0,
        "linebreak-style": 0,
        "react/jsx-filename-extension": 0 
    }
}